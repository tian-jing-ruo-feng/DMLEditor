import { projectService } from '@/utils/indexDB'

// 创建新项目
const newProject = {
  name: '新项目',
  description: '项目描述',
  createdAt: dayjs().format('YYYY-MM-DD HH:mm:ss'),
  content: JSON.stringify({/* 项目数据 */})
}
const projectId = await projectService.addProject(newProject)

// 获取所有项目
const allProjects = await projectService.getAllProjects()

// 更新项目
await projectService.updateProject(projectId, { name: '更新后的名称' })

// 删除项目
await projectService.deleteProject(projectId)
